{
  "name": "bolo-ui",
  "version": "0.1.3",
  "description": "A collection of UI components for Next.js projects",
  "main": "lib/index.js",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "storybook": "npx sb dev",
